{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default notice validation schema",
  "description": "Default validation schema",
  "required": ["debtor", "payee", "notice" ],
  "properties": {
    "debtor": {
      "type": "object"
    },
    "payee": {
      "type": "object"
    },
    "notice": {
      "type": "object",
      "required": ["qrCode"],
      "properties": {
        "qrCode": {
          "type": "string"
        }
      }
    }
  }
}