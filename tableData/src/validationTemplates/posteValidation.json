{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Poste notice validation schema",
  "description": "Poste notice validation schema",
  "required": ["debtor", "payee", "notice" ],
  "properties": {
    "debtor": {
      "type": "object"
    },
    "payee": {
      "type": "object"
    },
    "notice": {
      "type": "object",
      "required": ["posteAuth","posteAccountNumber","qrCode","posteDocumentType","posteDataMatrix"],
      "properties": {
        "posteAuth": {
          "type": "string"
        },
        "posteAccountNumber": {
          "type": "string"
        },
        "posteDocumentType": {
          "type": "string"
        },
        "posteDataMatrix": {
          "type": "string"
        },
        "qrCode": {
          "type": "string"
        }
      }
    }
  }
}