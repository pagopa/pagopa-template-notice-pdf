{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Installments notice validation schema",
  "description": "Installments notice validation schema",
  "required": ["debtor", "creditorInstitution", "notice" ],
  "properties": {
    "debtor": {
      "type": "object"
    },
    "creditorInstitution": {
      "type": "object"
    },
    "notice": {
      "type": "object",
      "required": ["dueDate","installments"],
      "properties": {
        "dueDate": {
          "type": "string"
        },
        "installments": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "dueDate"
            ],
            "properties": {
              "dueDate": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}